<% if spree_current_user %>
  <li class='link-to-account'><%= link_to Spree.t(:my_account), spree.account_path %></li>
  <li class='link-to-account'><%= link_to Spree.t(:logout), spree.logout_path %></li>
<% else %>
  <li id="link-to-login" class='link-to-account'><%= link_to Spree.t(:login), spree.login_path %></li>
<% end %>
